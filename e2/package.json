{
  "name": "mustang",
  "version": "0.8.23-dev",
  "description": "Mustang - Video conference, chat, mail, calendar, files, contacts",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build-org": "npm run typecheck && electron-vite build",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && npm run icon:icns && electron-builder --linux --config",
    "build:release:win": "npm run build && electron-builder --win --config --publish always",
    "build:release:mac": "npm run build && electron-builder --mac --config --publish always",
    "build:release:linux": "npm run build && npm run icon:icns && electron-builder --linux --config --publish always",
    "postinstall": "electron-builder install-app-deps",
    "icon:icns": "npx icon-gen --input build/icon.png --output build --icns --icns-name icon --icns-sizes 16,32,48,64,128,256,512"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "adm-zip": "^0.5.12",
    "better-sqlite3": "^11.2.1",
    "bufferutil": "^4.0.8",
    "electron-updater": "^6.3.3",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@sentry/vite-plugin": "^2.22.7",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^18.17.5",
    "electron": "^32.0.1",
    "electron-builder": "^24.6.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-svelte": "^2.32.4",
    "i18n": "^0.8.3",
    "icon-gen": "^5.0.0",
    "node-gyp": "^11.1.0",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-conditional-compile": "^1.4.5",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mustang-im/mustang.git"
  },
  "bugs": {
    "url": "https://github.com/mustang-im/mustang/issues"
  },
  "author": "Ben Bucksch and Mustang GmbH (https://mustang.im)",
  "copyright": "Ben Bucksch",
  "license": "EUPL-1.2",
  "licenseText": "See LICENSE file at top level",
  "homepage": "https://mustang.im"
}
