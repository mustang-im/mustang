{
  "name": "e2",
  "version": "1.0.0",
  "description": "An Electron application with Svelte and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0"
  },
  "devDependencies": {
    "jpc-ws": "^0.2.2",
    "@buttercup/credentials": "^1.2.0",
    "@xmldom/xmldom": "^0.8.10",
    "adm-zip": "^0.4.13",
    "emailjs-imap-client": "^3.0.7",
    "emailjs-mime-builder": "^2.0.5",
    "emailjs-mime-parser": "^2.0.7",
    "emailjs-smtp-client": "^2.0.1",
    "generate-password": "^1.4.1",
    "html-to-text": "^5.1.1",
    "i18n": "^0.8.3",
    "iocane": "^1.0.2",
    "r2": "^2.0.1",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.3",
    "svelte-collections": "^1.1.20",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@types/node": "^18.17.5",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-svelte": "^2.32.4",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
